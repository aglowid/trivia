<p id="notice"><%= notice %></p>

<h1>User Game Ques And Ans</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Game</th>
      <th>Question</th>
      <th>Answer</th>
      <th>Is correct ans</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_game_ques_and_ans.each do |user_game_ques_and_an| %>
      <tr>
        <td><%= user_game_ques_and_an.user_id %></td>
        <td><%= user_game_ques_and_an.game_id %></td>
        <td><%= user_game_ques_and_an.question_id %></td>
        <td><%= user_game_ques_and_an.answer %></td>
        <td><%= user_game_ques_and_an.is_correct_ans %></td>
        <td><%= link_to 'Show', user_game_ques_and_an %></td>
        <td><%= link_to 'Edit', edit_user_game_ques_and_an_path(user_game_ques_and_an) %></td>
        <td><%= link_to 'Destroy', user_game_ques_and_an, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Game Ques And An', new_user_game_ques_and_an_path %>
